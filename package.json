{
  "name": "web-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "daemon": "npm run build && pm2 start \"npm run start\" --name web-client",
    "daemon:re": "npm run build && pm2 stop web-client && pm2 start \"npm run start\" --name web-client",
    "daemon:stop": "pm2 stop web-client",
    "daemon:log": "pm2 logs web-client --lines 1000",
    "daemon:ssl": "npm run build && pm2 start \"npm run start:ssl\" --name web-client-ssl",
    "daemon:ssl:re": "npm run build && pm2 stop web-client && pm2 start \"npm run start:ssl\" --name web-client-ssl",
    "daemon:ssl:stop": "pm2 stop web-client-ssl",
    "dev": "npm run lint && next dev -p 3000",
    "build": "next build",
    "start": "next start -p 8080",
    "start:ssl": "next start -p 443",
    "lint": "eslint ./ --ext .tsx,.ts --no-error-on-unmatched-pattern --fix --quiet"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fullcalendar/core": "^6.1.15",
    "@fullcalendar/daygrid": "^6.1.15",
    "@fullcalendar/interaction": "^6.1.15",
    "@fullcalendar/react": "^6.1.15",
    "@nextui-org/react": "^2.4.8",
    "@prisma/client": "^5.8.1",
    "@reduxjs/toolkit": "^2.0.1",
    "browser-image-compression": "^2.0.2",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.44",
    "next": "14.0.4",
    "next-redux-wrapper": "^8.1.0",
    "react": "^18",
    "react-daum-postcode": "^3.1.3",
    "react-dom": "^18",
    "react-moment": "^1.1.3",
    "react-redux": "^9.1.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
